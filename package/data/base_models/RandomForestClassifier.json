{                  
    "model_base": "sklearn",
    "model_module": "sklearn.ensemble",
    "model_class" : "RandomForestClassifier",
    "RandomForestClassifier" : {
        "Hyperparameters" : {
            "n_estimators" : {
                "type" : "int",
                "default" : 100,
                "min" : 10,
                "max" : 200,
                "step_size" : 10,
                "tunable" : true,
                "tooltip" : ""
            },
            "max_features" : {
                "type" : "dropdown",
                "default" : "auto",
                "options" : {
                    "auto" : "auto",
                    "sqrt" : "sqrt",
                    "log2" : "log2"
                },
                "tunable" : true,
                "tooltip" : ""
            },
            "max_depth" : {
                "type" : "int",
                "default" : 1000,
                "min" : 1,
                "max" : 1000,
                "step_size" : 1,
                "tunable" : false
            },
            "min_samples_split" : {
                "type" : "int",
                "default" : 2,
                "min" : 2,
                "max" : 10,
                "step_size" : 1,
                "tunable" : true,
                "tooltip" : ""
            },
            "bootstrap" : {
                "type" : "dropdown",
                "default" : true,
                "options" : {
                    "False" : false,
                    "True" : true
                },
                "tunable" : true,
                "tooltip" : ""
            },
            "n_jobs" : {
                "type" : "static",
                "default" : 1,
                "tunable" : false,
                "tooltip" : "For RFC, this must be set to 1 to prevent threading issues."
            }
        }
    }
}