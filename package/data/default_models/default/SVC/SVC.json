{
  "model_base": "sklearn",
  "model_module": "sklearn.svm",
  "model_class": "SVC",
  "question_number": "default",
  "version": "default",
  "meta": {
    "training_meta": {},
    "tuning_meta": {}
  },
  "params": {
    "sklearn.svm.SVC": {
      "C": 1.0,
      "shrinking": false,
      "probability": true,
      "tol": 0.001,
      "cache_size": 200,
      "class_weight": null,
      "kernel": "linear",
      "gamma": "scale"
    },
    "sklearn.feature_extraction.text.TfidfVectorizer": {
      "ngram_range": [
        1,
        2
      ],
      "encoding": "utf-8",
      "strip_accents": 0,
      "max_df": 1.0,
      "min_df": 1,
      "use_idf": true
    },
    "sklearn.feature_selection.SelectPercentile": {
      "percentile": 10,
      "score_func": "f_classif"
    }
  }
}