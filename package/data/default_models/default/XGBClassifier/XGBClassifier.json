{
  "model_base": "sklearn",
  "model_module": "xgboost.sklearn",
  "model_class": "XGBClassifier",
  "question_number": "default",
  "version": "default",
  "meta": {
    "training_meta": {},
    "tuning_meta": {}
  },
  "params": {
    "xgboost.sklearn.XGBClassifier": {
      "n_estimators": 100,
      "max_features": "auto",
      "gamma": 0.0,
      "subsample": 1.0,
      "colsample_bytree": 1.0,
      "reg_alpha": 0.0,
      "reg_lambda": 1.0
    },
    "sklearn.feature_extraction.text.TfidfVectorizer": {
      "ngram_range": [
        1,
        2
      ],
      "encoding": "utf-8",
      "strip_accents": 0,
      "max_df": 1.0,
      "min_df": 1,
      "use_idf": true
    },
    "sklearn.feature_selection.SelectPercentile": {
      "percentile": 10,
      "score_func": "f_classif"
    }
  }
}